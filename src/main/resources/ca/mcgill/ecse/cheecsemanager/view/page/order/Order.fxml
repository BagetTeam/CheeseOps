<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.HBox?>
<?import org.controlsfx.control.BreadCrumbBar?>
<?import javafx.scene.control.Separator?>

<VBox xmlns:fx="http://javafx.com/fxml"
      fx:controller="ca.mcgill.ecse.cheecsemanager.fxml.controller.order.OrderController"
      fx:id="parentContainer"
      spacing="16">
    <children>
        <HBox>
            <BreadCrumbBar styleClass="breadcrumb" fx:id="breadCrumbBar"/>
        </HBox>
        <Separator />
        <VBox fx:id="childContainer" VBox.vgrow="ALWAYS"/>
    </children>
    <padding>
        <Insets bottom="16.0" left="16.0" right="16.0" top="16.0"/>
    </padding>
</VBox>
