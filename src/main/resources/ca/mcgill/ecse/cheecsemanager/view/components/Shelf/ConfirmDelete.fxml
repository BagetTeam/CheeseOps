<?xml version="1.0" encoding="UTF-8" ?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import ca.mcgill.ecse.cheecsemanager.fxml.components.StyledButton?>
<VBox
    spacing="16"
    xmlns="http://javafx.com/javafx"
    xmlns:fx="http://javafx.com/fxml"
    fx:controller="ca.mcgill.ecse.cheecsemanager.fxml.controllers.shelf.ConfirmDeletePopUpController"
    fx:id="root"
    styleClass="w-sm"
>
    <Label wrapText="true" styleClass="text-fg" fx:id="warningLabel" />
    <Label wrapText="true" styleClass="text-destructive" fx:id="errorLabel" visible="false" managed="false" />
    <HBox spacing="12" alignment="CENTER_RIGHT">
        <StyledButton text="No" variant="MUTED" fx:id="noBtn" />
        <StyledButton text="Delete" variant="DESTRUCTIVE" fx:id="yesBtn" />
    </HBox>
</VBox>
